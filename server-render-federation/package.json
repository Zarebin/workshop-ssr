{
  "name": "server-render",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "buildPlatform": "npm-run-all --parallel platform:build*",
    "startPlatform": "nodemon --watch ui-platform/build --exec \"node ui-platform/build/bundle.js\"",
    "platform:build-server": "webpack --config ui-platform/config/webpack.server.js --watch",
    "platform:build-client": "webpack --config ui-platform/config/webpack.client.js --watch",
    "buildRemote": "webpack --config=remoteServer/webpack.config.js -w",
    "buildRemoteClient": "webpack --config=remoteServer/webpack.client.js -w",
    "startRemote": "nodemon remoteServer/public/server/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "babel-loader": "8.2.2",
    "file-loader": "^6.2.0",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "5.48.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "0.21.1",
    "compression": "1.7.4",
    "concurrently": "6.2.0",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "^7.2.4",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "6.0.0"
  }
}
